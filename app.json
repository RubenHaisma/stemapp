{
  "expo": {
    "name": "De StemAPP",
    "slug": "de-stemapp",
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./public/logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rubenyest.destemapp",
      "appleTeamId": "M2379FZ846"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "package": "com.rubenyest.destemapp"
    }
  }
}
